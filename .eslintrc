{
  "extends": ["eslint:recommended","prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "no-undef": "off",
    "node/no-unpublished-require": ["error", {
      "allowModules": ["dotenv"]
      }
    ]
  }
}